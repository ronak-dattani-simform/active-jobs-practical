<div class="container">
<table class="table">
  <thead>
    <tr>
      <th scope="col">#id</th>
      <th scope="col">event Name</th>
      <th scope="col">Description</th>
      <th scope="col">Date</th>      
    </tr>
  </thead>
  <tbody>
  <% @events.each do |event|%>
    <tr>
      <th scope="row"><%= event.id %></th>
      <td><%= event.title %></td>
      <td><%= event.description %></td>
      <td><%= event.event_time %></td>
      <% if ! event.id.in?(@registered_events)%>
      <td><%= link_to "Enroll  ", event_enroll_path(event.id), class: "btn btn-primary" %></td>
      <% else %>
      <td><%= link_to "Deroll", event_unenroll_path(event.id), class: "btn btn-danger" %></td>
      <% end %>
    </tr>
  <%end %>

  </tbody>
</table>
</div>