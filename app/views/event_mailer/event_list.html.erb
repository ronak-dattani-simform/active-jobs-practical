<div class="container">
<h1> Registered Events </h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#id</th>
      <th scope="col">event Name</th>
      <th scope="col">Description</th>
      <th scope="col">Date</th>      
    </tr>
  </thead>
  <tbody>
  <% @events.each do |event|%>
    <tr>
      <th scope="row"><%= event.id %></th>
      <td><%= event.title %></td>
      <td><%= event.description %></td>
      <td><%= event.event_time %></td>
  <%end %>
  </tbody>
</table>
</div>